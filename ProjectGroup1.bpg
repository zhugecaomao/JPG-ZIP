#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = JPG_RAR.exe JPG_RAR.CHS JPG_RAR.ENU
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

JPG_RAR.exe: JPG_RAR.dpr
  $(DCC)

JPG_RAR.CHS: CHS\JPG_RAR.dpr
  $(DCC)

JPG_RAR.ENU: ENU\JPG_RAR.dpr
  $(DCC)


